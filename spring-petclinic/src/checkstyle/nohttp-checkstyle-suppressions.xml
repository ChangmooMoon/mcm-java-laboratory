<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
	"-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
	"https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
	<suppress checks=".*" files="node_modules/.*"/>
	<suppress checks=".*" files="node/.*"/>
	<suppress checks=".*" files="build/.*"/>
	<suppress checks="NoHttp" files="[\\/]build.log"/>
	<suppress checks="NoHttp" files=".+\.(jar|git|ico|p12|gif|jks|jpg|svg)"/>
</suppressions>
